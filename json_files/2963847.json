        {
            "order": 153,
            "start": 573.5799999999999,
            "end": 578.5999999999999,
            "transcription": " A última atualização foi, inclusive, há 10 dias."
        },
        {
            "order": 154,
            "start": 579.9599999999999,
            "end": 581.64,
            "transcription": " Foi tranquilo, né, Chirio?"
